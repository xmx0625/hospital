<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c" %>
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
    <title>添加收费项目-- -2015</title>
    <meta charset="UTF-8">
    <link rel="stylesheet" type="text/css" href="../Css/bootstrap.css" />
    <link rel="stylesheet" type="text/css" href="../Css/bootstrap-responsive.css" />
    <link rel="stylesheet" type="text/css" href="../Css/style.css" />
    <script type="text/javascript" src="../Js/jquery.js"></script>
    <script type="text/javascript" src="../Js/jquery.sorted.js"></script>
    <script type="text/javascript" src="../Js/bootstrap.js"></script>
    <script type="text/javascript" src="../Js/ckform.js"></script>
    <script type="text/javascript" src="../Js/common.js"></script>
    <script type="text/javascript" src="../Js/ckeditor/ckeditor.js"></script>
 

    <style type="text/css">
        body {
            padding-bottom: 40px;
        }
        .sidebar-nav {
            padding: 9px 0;
        }

        @media (max-width: 980px) {
            /* Enable use of floated navbar text */
            .navbar-text.pull-right {
                float: none;
                padding-left: 5px;
                padding-right: 5px;
            }
        }


    </style>
    <script type="text/javascript">
    $(function () {       
		$('#backid').click(function(){
				window.location.href="charge.html";
		 });
    });
    </script>
</head>
<body>

<table class="table table-bordered table-hover definewidth m10">
	
    <tr>
        <td width="10%" class="tableleft">病历号</td>
        <td>${medical_record }</td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">姓名</td>
        <td>${rname }</td>
    </tr>
    
    <tr>
        <td width="10%" class="tableleft">收费项目</td>
        <td>
        <form action="${pageContext.request.contextPath }/charge/se">
        	<select name="pay_items_id">
        		<option value="1">心脏检查</option>
        		<option value="2">CT</option>
        		<option value="3">尿检</option>
        		<option value="4">抽血化验</option>
        		<option value="5">一般手术费用</option>
        		<option value="6">中等难度手术费用</option>
        		<option value="7">肝脏检查</option>
        		<option value="8">肾脏检查</option>
        		<option value="9">视力检查</option>
        		<option value="10">神经外科检查</option>
        		<option value="11">耳鼻喉科检查</option>
        	</select>
        </form>
        </td>
    </tr>
    <form action="${pageContext.request.contextPath }/charge/add" method="post">
    <tr>
        <td width="10%" class="tableleft">收费金额</td>
        <td>
        
        	<input type="hidden" name="medical_record" value="${medical_record }">
        	<input type="hidden" name="pay_items_id" value="${pay_items_id }">
        	<input type="text" name="charge_money" value="${charge_money }"/>
        
        </td>
    </tr>
    
    <tr>
        <td colspan="2">
			<center>
				<button type="submit" class="btn btn-primary" type="button">保存</button> &nbsp;&nbsp;<button type="button" class="btn btn-success" name="backid" id="backid">返回列表</button>
			</center
		</td>
    </tr>
    </form>
