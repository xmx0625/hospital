<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <title>修改住院信息-- -2015</title>
    <meta charset="UTF-8">
    <link rel="stylesheet" type="text/css" href="../Css/bootstrap.css" />
    <link rel="stylesheet" type="text/css" href="../Css/bootstrap-responsive.css" />
    <link rel="stylesheet" type="text/css" href="../Css/style.css" />
    <script type="text/javascript" src="../Js/jquery.js"></script>
    <script type="text/javascript" src="../Js/jquery.sorted.js"></script>
    <script type="text/javascript" src="../Js/bootstrap.js"></script>
    <script type="text/javascript" src="../Js/ckform.js"></script>
    <script type="text/javascript" src="../Js/common.js"></script>
    <script type="text/javascript" src="../Js/ckeditor/ckeditor.js"></script>
 

    <style type="text/css">
        body {
            padding-bottom: 40px;
        }
        .sidebar-nav {
            padding: 9px 0;
        }

        @media (max-width: 980px) {
            /* Enable use of floated navbar text */
            .navbar-text.pull-right {
                float: none;
                padding-left: 5px;
                padding-right: 5px;
            }
        }


    </style>
    <script type="text/javascript">
    $(function () {       
		$('#backid').click(function(){
				window.location.href="${pageContext.request.contextPath}/hospital/list";
		 });
    });
    </script>
</head>
<body>
<form action="${pageContext.request.contextPath}/hospital/edit" method="post" class="definewidth m20">
	<input type="hidden" name="medical_record" value="${reg.medical_record }">
<table class="table table-bordered table-hover definewidth m10">
    <tr>
        <td width="10%" class="tableleft">病历号</td>
        <td>${reg.medical_record }</td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">姓名</td>
        <td>${reg.name }</td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">证件类型</td>
        <td><select name="certificate_type">
        <option value="1" ${reg.certificate_type==1?"selected":"" }>身份证</option>
        <option value="2" ${reg.certificate_type==2?"selected":"" }>护照</option>
        <option value="3" ${reg.certificate_type==3?"selected":"" }>军人证</option>
        </select></td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">证件号</td>
        <td>${reg.ID_num }</td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">社保号</td>
        <td>${reg.social_safe_num }</td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">联系电话</td>
        <td>${reg.phone }</td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">是否自费</td>
        <td><input type="radio" name="self_paying" value="" ${reg.self_paying==0?"checked":""}/>否&nbsp;&nbsp;&nbsp;
        <input type="radio" name="self_paying" value="" ${reg.self_paying==1?"checked":""}/>是</td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">性别</td>
        <td><input type="radio" name="sex" value="" ${reg.sex==1?"checked":""}/>男&nbsp;&nbsp;&nbsp;
        <input type="radio" name="sex" value="" ${reg.sex==0?"checked":""}/>女</td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">年龄</td>
        <td>${reg.age }</td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">初复诊</td>
        <td><input type="radio" name="early_appointment" value="" ${reg.early_appointment eq "初诊"?"checked":""}/>初诊&nbsp;&nbsp;&nbsp;
        <input type="radio" name="early_appointment" value="" ${reg.early_appointment eq "复诊"?"checked":""}/>复诊</td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">所挂科室</td>
        <td><select name="section_id">
        <option value="1" ${reg.section_id==1?"selected":"" }>血液科</option>
        <option value="2" ${reg.section_id==2?"selected":"" }>骨科</option>
        <option value="3" ${reg.section_id==3?"selected":"" }>外科</option>
        <option value="4" ${reg.section_id==4?"selected":"" }>急诊科</option>
        <option value="5" ${reg.section_id==5?"selected":"" }>妇科</option>
        </select></td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">指定医生</td>
        <td><select name="doctor_id">
        <option value="1" ${reg.doctor_id==1?"selected":"" }>华佗</option>
        <option value="2" ${reg.doctor_id==2?"selected":"" }>扁鹊</option>
        <option value="3" ${reg.doctor_id==3?"selected":"" }>李时珍</option>
        <option value="4" ${reg.doctor_id==4?"selected":"" }>张仲景</option>
        </select></td>
    </tr>
	<tr>
        <td width="10%" class="tableleft">备注</td>
        <td><textarea>${reg.noted }</textarea></td>
	</tr>

    <tr>
        <td width="10%" class="tableleft">护理</td>
        <td><input type="text" name="caregiver" value="${reg.hospitalInfor.caregiver }"/></td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">床位号</td>
        <td><input type="text" name="bed_id" value="${reg.hospitalInfor.bed_id }"/></td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">缴费押金</td>
        <td><input type="text" name="pay_the_deposit" value="${reg.hospitalInfor.pay_the_deposit }"/></td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">病情</td>
        <td><textarea name="state_illness">${reg.hospitalInfor.state_illness }</textarea></td>
    </tr>
    <tr>
        <td colspan="2">
			<center>
				<button type="submit" class="btn btn-primary" type="button">保存</button> &nbsp;&nbsp;<button type="button" class="btn btn-success" name="backid" id="backid">返回列表</button>
				<span>${msg }</span>
			</center>
		</td>
    </tr>
</table>
